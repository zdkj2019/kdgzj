<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />

    <style name="AppTheme" parent="@style/AppBaseTheme" />

    <style name="table_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginLeft">0.5dp</item>
        <item name="android:layout_marginRight">0.5dp</item>
        <item name="android:background">#ACCBF0</item>
    </style>

    <style name="label">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:padding">10.0sp</item>
    </style>

    <style name="blacktext">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="toplogout">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ff0680ed</item>
    </style>

    <style name="confirm">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="toptitle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="showuserid">
        <item name="android:textSize">29.0sp</item>
        <item name="android:textColor">#ff0680ed</item>
    </style>

    <style name="mydialog">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ff0190eb</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:button">@drawable/checkbox</item>
        <item name="android:maxHeight">10.0sp</item>
    </style>

    <style name="confirminfo">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ff0190eb</item>
    </style>

    <style name="label1">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="RadioButton_appraise" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:paddingLeft">80.0sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@drawable/appraise_radio</item>
        <item name="android:maxHeight">16.0sp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="dlg_radiobutton" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">20.0sp</item>
        <item name="android:paddingLeft">100.0sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
        <item name="android:button">@drawable/dlg_radio</item>
        <item name="android:maxHeight">10.0sp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="content">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="main_ss">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0.5dip</item>
    </style>

    <style name="tablerow_sty">
        <item name="android:paddingLeft">3.0dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="text_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/right_ico</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="edittext_sty">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/edittext_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="view_sty">
        <item name="android:background">#ffb8b8b8</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dip</item>
    </style>

    <style name="dot_style">
        <item name="android:layout_width">5dip</item>
        <item name="android:layout_height">5dip</item>
        <item name="android:background">@drawable/dot_normal</item>
        <item name="android:layout_marginLeft">1.5dip</item>
        <item name="android:layout_marginRight">1.5dip</item>
    </style>

    <style name="ProgressBarStyle" parent="@android:style/Widget.ProgressBar.Horizontal" />

</resources>